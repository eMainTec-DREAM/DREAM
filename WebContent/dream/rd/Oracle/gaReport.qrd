                                AND a.eqloc_id = x.eqloc_id
                                AND a.mtlnpoint = 'BD_HOURS'
                                AND a.yyyymm like substr('^param3',0,4)||'__')/
                                DECODE((SELECT count(*)
                                            FROM TAMTLNPOINT a
                                            WHERE 1=1
                                            AND a.plant = x.plant
                                            AND a.comp_no = x.comp_no
                                            AND a.eqloc_id = x.eqloc_id
                                            AND a.mtlnpoint = 'BD_HOURS'
                                            AND a.yyyymm like substr('^param3',0,4)||'__'),0,null,(SELECT count(*)
                                                                                                FROM TAMTLNPOINT a
