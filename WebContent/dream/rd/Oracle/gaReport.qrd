                and b.eqloc_id in (1001,1034,1045)
                group by a.comp_no, a.wrk_date,a.plant),0)||'%' ead
FROM TADAY x
where x.tmonth  = substr('^param3',0,4)||substr('^param3',6,2)
and x.tday IN ( SELECT cal_date FROM TAWRKCALENDAR 
