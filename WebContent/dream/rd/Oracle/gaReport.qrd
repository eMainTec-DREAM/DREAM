$
SELECT 
		 TO_CHAR(to_date('^param3'||'01','yyyy-mmdd'), 'MONTH YYYY', 'NLS_DATE_LANGUAGE=ENGLISH') mmyyyy
		,TO_CHAR(sysdate,'yyyy. mm. dd') yyyymmdd
from dual
/
