            and a.wrk_date  = x.tday
            and a.plant = '^param5'
            and a.comp_no = '^param1'
            and b.eqloc_id = 1045),0) as_fq
        , NVL((select to_char(round(DECODE(nvl(d_repairtime,0),0,null,nvl(d_repairtime,0))/a.d_worktime*100,2),'FM9999990D00') from TAKPIDLOCDN a, TAEQLOC b
